{
  "name": "decision-tree-maker",
  "private": true,
  "description": "Decision tree maker",
  "version": "0.0.1",
  "workspaces": [
    "examples/*",
    "packages/*"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "format:check": "prettier --check './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "preversion": "yarn link && yarn format:check",
    "prepare": "husky install"
  },
  "keywords": [
    "Decision",
    "tree"
  ],
  "author": "Damien Le Dantec <damien-le-dantec@hotmail.fr>",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "husky": "^8.0.1",
    "prettier": "^2.7.1"
  }
}
